{
	"Default": 
	{
		"$type": "ConfigAbility",
		"abilityName": "Monster_Abyss_Electric_Pose",
		"abilityMixins":
		[
			// _ABILITY_ABYSS_POSETAG_
			// 0:有盾
			// 1:没盾元素缺失
			// 2:没盾元素聚集
			{
				"$type": "AttachModifierToSelfGlobalValueMixin",
				"globalValueKey": "_ABILITY_ABYSS_POSETAG_",
				"valueSteps": [-0.5, 0.5, 1.5, 2.5],
				"defaultGlobalValueOnCreate":0,
				"removeAppliedModifier": true,
				"modifierNameSteps": ["Pose_HasElement","Pose_DetectElement","Pose_GatherElement"],
			},
		],
		"modifiers":
		{
			"Pose_HasElement":
			{
				"onAdded":
				[
					{
						"$type":"SetPoseBool",
						"boolID":"ElementState",
						"value":true,
					},
				],
				"onRemoved":
				[
					{
						"$type":"SetPoseBool",
						"boolID":"ElementState",
						"value":false,
					},
				],
			},
			"Pose_DetectElement":
			{
				"onAdded":
				[
					{
						"$type":"SetPoseBool",
						"boolID":"DetectEnd",
						"value":false,
					},
					{
						"$type":"SetPoseBool",
						"boolID":"ElementState",
						"value":false,
					},
				],
				"onRemoved":
				[
					{
						"$type":"SetPoseBool",
						"boolID":"DetectEnd",
						"value":true,
					},
				],
			},
			"Pose_GatherElement":
			{
				"onAdded":
				[
					{
						"$type":"SetPoseBool",
						"boolID":"GatherEnd",
						"value":false,
					},
					{
						"$type":"SetPoseBool",
						"boolID":"ElementState",
						"value":false,
					},
				],
				"onRemoved":
				[
					{
						"$type":"SetPoseBool",
						"boolID":"GatherEnd",
						"value":true,
					},
				],
			},
		},
	},
},
