					// 这块的打断处理没想到更合理的方法了，等其他深渊法师如果有幸重构的话，学习一下
					{
						"$type": "SetGlobalValue",
						"key": "_ABILITY_ABYSS_POSETAG_",
						"value": 1,
					},
					{
						"$type":"SetPoseBool",
						"boolID":"GatherBreak",
						"value":true,
					},
				],
			},
		},
	},
